혹은 다음으로도 처리 가능.
var newData = [
            {id: '78', thingy: "jkl"}
        ];
        var rowId = $.jgrid.randId(); // new row ID
        $("#list").jqGrid('addRowData', rowId, newData);


$("#list").addRowData(rowid);
=>$("#list").jqGrid('addRowData',rowid);

$("#list").delRowData(rowid);
=>$("#list").jqGrid('delRowData',rowid);




jq("#list").setSelection(top_rowid);
=>jq("#list").jqGrid('setSelection', top_rowid);


var rowData=jQuery("#list").getRowData(rowId);
=>var rowData=jQuery("#list").jqGrid('getRowData', rowId);


$("#list").setGridHeight(<%=listHeight %>);
=> $("#list").jqGrid('setGridHeight', <%=listHeight %>);

jq("#list2").clearGridData();
=>jq("#list2").jqGrid('clearGridData');

jq("#list2").saveRow(rowid);
=>jq("#list2").jqGrid('saveRow', rowid);

jq("#list2").editRow(rowid);
=>jq("#list2").jqGrid('editRow', rowid);
