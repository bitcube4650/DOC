
JQGRID 버전 업그레이드
var rowCnt = $("#list2").getGridParam('reccount');
=>var rowCnt = $("#list2").jqGrid('getGridParam','records');


$(this).setCell(rowid,'TITLE','',{color:'#0000ff',cursor:'pointer'});
=>$("#list2").jqGrid('setCell', rowid,'TITLE','',{color:'#0000ff',cursor:'pointer'});

var rowid = $("#list").getDataIDs()[i];
=>  var $ids=$("#list").jqGrid('getDataIDs'); 
for($i=0;$i<$ids.length;$i++){

}




$("#list").addRowData(rowid,{ 
		ATTACH_SEQ		 : rtn_attach_seq, 
